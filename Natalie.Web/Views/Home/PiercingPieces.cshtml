@{ Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "Natalie Macellaio | Piercing Pieces";
}
<div class="mastwrap">
    <section class="page-section" id="piercingpieces">
        <div class="container">

            <div class="row">
                <article class="col-md-12 showcase-single-content white-bg">
                    <div class="row">
                        <article class="col-md-8 text-left showcase-single-item-wrap white-bg">
                            <div class="showcase-single-item">
                                <div class="showcase-single-item-inner text-left">
                                    <p><span>Wall installation</span></p>
                                    <h1 class="font1 color add-top-quarter"><span>a memory</span></h1>
